<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet-mapping>
		<servlet-name>MealMenuFeedRouter</servlet-name>
		<url-pattern>/feed/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>MealMenuFeedRouter</servlet-name>
		<servlet-class>com.mindflakes.TeamRED.server.MealMenuFeedRouter</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>MealMenuUpcomingFeedServlet</servlet-name>
		<servlet-class>com.mindflakes.TeamRED.server.MealMenuUpcomingFeedServlet</servlet-class>
	</servlet>
	
	
	<welcome-file-list>
		<welcome-file>upcoming.jsp</welcome-file>
	</welcome-file-list>
</web-app>
